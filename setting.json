        "input_dim": 64,
        "output_dim": 64,
        "max_seq_len": 20,
        "n_layers": 2
    },

    "lstm_attn": {
        "embedding_dim": 21,
        "input_dim": 64,
        "output_dim": 64,
        "max_seq_len": 20,
        "n_layers": 2,
        "n_heads": 2,
        "drop_out": 0.2
    },
    "bert": {
        "embedding_dim": 21,
        "input_dim": 64,

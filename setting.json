    "scheduler": "plateau",

    "epoch": 10,

    "batch_size": 16,

    "num_workers": 1,

    "wandb_activate": true,

    "adam": {"learn_rate": 0.01, "weight_decay": 0},

    "plateau": {"patience": 10, "factor": 0.5, "mode": "max", "verbose": true},

    "lstm": {

{
    "path": {
        "data": "data",
        "code": "code",
        "log": "log",
        "model": "model",
        "state_dict": "state_dict",
        "submit": "submit",
        "train": "train",
        "valid": "valid",
        "ensemble": "ensemble"
    },

    "file_name": {
        "train": "train_data",
        "test": "test_data"
    },


    "seed": 42,
    "device": "cuda",

    "fixed_train_columns": [
        "userID",
        "assessmentItemID",
        "answerCode"
    ],

    "fixed_embedding_columns": [
        "assessmentItemID"
    ],

    "selective_train_columns": [
        "testId",
        "KnowledgeTag"
    ],

    "selective_embedding_columns": [
        "testId",
        "KnowledgeTag"
    ],

    "predict_column": "answerCode",


    "train_valid_split": 0.99,


    "model_name": "lgcn",
    "loss_fn": "BCEWLL",
    "optimizer": "ADAM",
    "scheduler": "plateau",
    "epoch": 10,
    "batch_size": 16,
    "num_workers": 1,
    "wandb_activate" : 1, 


    "adam": {
        "learn_rate": 0.01,
        "weight_decay": 0
    },


    "plateau":   {
        "patience": 10,
        "factor": 0.5,
        "mode": "max",
        "verbose": true
    },


    "lstm": {
        "embedding_dim": 21,
        "input_dim": 64,
        "output_dim": 64,
        "max_seq_len": 20,
        "n_layers": 2
    },

    "lstm_attn":    {
        "embedding_dim": 21,
        "input_dim": 64,
        "output_dim": 64,
        "max_seq_len": 20,
        "n_layers": 2,
        "n_heads": 2,
        "drop_out": 0.2
    },

    "bert": {
        "embedding_dim": 21,
        "input_dim": 64,
        "max_seq_len": 20,
        "n_layers": 2,
        "n_heads": 2
    },

    "lgcn": {
        "embedding_dim": 64,
        "num_layers": 1,
        "alpha": 0
    }  
}
{
    "path": {
        "data": "data",
        "code": "code",
        "log": "log",
        "model": "model",
        "state_dict": "state_dict",
        "submit": "submit",
        "train": "train",
        "valid": "valid",
        "ensemble": "ensemble"
    },

    "file_name": {"train": "2023-05-22_train_data", "test": "2023-05-22_test_data"},

    "seed": 42,

    "device": "cuda",

    "fixed_train_columns": ["user_id", "question_id", "answer_code"],

    "fixed_embedding_columns": ["question_id"],

